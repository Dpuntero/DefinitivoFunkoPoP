
@{
    ViewBag.Title = "MisPedidos";
}



    <h2>Listado de pedidos</h2>
    <p>Listado de sus pedidos y su estado:</p>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Dirección</th>
                <th>Nº de pedido</th>
                <th>Artículos</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var prod in Model)
            {
                <tr>
                    <td>@prod.P_ID</td>
                    <td>@prod.UNIDADES</td>
                    <td>@prod.PRECIO</td>
                    <td>@prod.DESCUENTO</td>
                </tr>
            }
